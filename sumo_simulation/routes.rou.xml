<?xml version="1.0" encoding="UTF-8"?>
<!-- LifeLine AI - Vehicle Routes and Traffic Generation -->
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    
    <!-- Vehicle Types (60% SLOWER - Ultra slow speeds for excellent visualization) -->
    <vType id="car" vClass="passenger" speedFactor="0.0002" sigma="0.05" accel="0.8" decel="2.0" length="5" color="1,1,0" guiShape="passenger"/>
    <vType id="bus" vClass="bus" speedFactor="0.08" sigma="0.03" accel="0.5" decel="1.8" length="12" color="0,0.5,1" guiShape="bus"/>
    <vType id="bike" vClass="bicycle" speedFactor="0.08" sigma="0.05" accel="0.6" decel="1.5" length="2" color="0,0.8,0.8" guiShape="bicycle"/>
    <vType id="motorcycle" vClass="motorcycle" speedFactor="0.02" sigma="0.6" accel="1.0" decel="2.0" length="2.5" color="0.2,0.2,0.2" guiShape="motorcycle"/>
    <vType id="truck" vClass="truck" speedFactor="0.072" sigma="0.2" accel="0.4" decel="1.5" length="10" color="0.5,0.5,0.5" guiShape="truck"/>
    <vType id="ambulance" vClass="emergency" speedFactor="0.2" sigma="0.0" accel="2.0" decel="3.0" length="7" maxSpeed="12" color="1,0,0" guiShape="emergency" width="2.5" height="2.5" minGap="0"/>
    
    <!-- Route Definitions (Multiple paths through the grid) -->
    
    <!-- West to East Routes -->
    <route id="route_WE_0" edges="e00_01 e01_02 e02_03"/>
    <route id="route_WE_1" edges="e10_11 e11_12 e12_13"/>
    <route id="route_WE_2" edges="e20_21 e21_22 e22_23"/>
    <route id="route_WE_3" edges="e30_31 e31_32 e32_33"/>
    
    <!-- East to West Routes -->
    <route id="route_EW_0" edges="e03_02 e02_01 e01_00"/>
    <route id="route_EW_1" edges="e13_12 e12_11 e11_10"/>
    <route id="route_EW_2" edges="e23_22 e22_21 e21_20"/>
    <route id="route_EW_3" edges="e33_32 e32_31 e31_30"/>
    
    <!-- North to South Routes -->
    <route id="route_NS_0" edges="e00_10 e10_20 e20_30"/>
    <route id="route_NS_1" edges="e01_11 e11_21 e21_31"/>
    <route id="route_NS_2" edges="e02_12 e12_22 e22_32"/>
    <route id="route_NS_3" edges="e03_13 e13_23 e23_33"/>
    
    <!-- South to North Routes -->
    <route id="route_SN_0" edges="e30_20 e20_10 e10_00"/>
    <route id="route_SN_1" edges="e31_21 e21_11 e11_01"/>
    <route id="route_SN_2" edges="e32_22 e22_12 e12_02"/>
    <route id="route_SN_3" edges="e33_23 e23_13 e13_03"/>
    
    <!-- Complex Routes (turning paths) -->
    <route id="route_complex_1" edges="e00_01 e01_11 e11_21 e21_22 e22_23"/>
    <route id="route_complex_2" edges="e03_13 e13_12 e12_11 e11_10"/>
    <route id="route_complex_3" edges="e30_31 e31_21 e21_11 e11_01"/>
    <route id="route_complex_4" edges="e33_23 e23_22 e22_21 e21_20"/>
    
    <!-- Ambulance Routes (Different paths for 7 ambulances) -->
    <route id="ambulance_route_1" edges="e00_01 e01_11 e11_21 e21_31"/>
    <route id="ambulance_route_2" edges="e03_13 e13_12 e12_11 e11_10"/>
    <route id="ambulance_route_3" edges="e30_31 e31_21 e21_11 e11_01"/>
    <route id="ambulance_route_4" edges="e33_23 e23_22 e22_21 e21_20"/>
    <route id="ambulance_route_5" edges="e00_10 e10_20 e20_30"/>
    <route id="ambulance_route_6" edges="e10_11 e11_12 e12_22 e22_32"/>
    <route id="ambulance_route_7" edges="e20_21 e21_22 e22_23"/>
    
    <!-- Normal Traffic Flow Generation (Very High Density for Congestion) -->
    <!-- Cars - 50% of traffic -->
    <flow id="flow_cars_WE" type="car" route="route_WE_1" begin="0" end="1000" probability="0.12" departSpeed="random"/>
    <flow id="flow_cars_EW" type="car" route="route_EW_1" begin="0" end="1000" probability="0.12" departSpeed="random"/>
    <flow id="flow_cars_NS" type="car" route="route_NS_1" begin="0" end="1000" probability="0.12" departSpeed="random"/>
    <flow id="flow_cars_SN" type="car" route="route_SN_2" begin="0" end="1000" probability="0.12" departSpeed="random"/>
    
    <flow id="flow_cars_complex_1" type="car" route="route_complex_1" begin="0" end="1000" probability="0.08"/>
    <flow id="flow_cars_complex_2" type="car" route="route_complex_2" begin="0" end="1000" probability="0.08"/>
    <flow id="flow_cars_WE_2" type="car" route="route_WE_0" begin="0" end="1000" probability="0.10"/>
    <flow id="flow_cars_NS_2" type="car" route="route_NS_3" begin="0" end="1000" probability="0.10"/>
    
    <!-- Buses - 15% of traffic -->
    <flow id="flow_bus_WE" type="bus" route="route_WE_2" begin="0" end="1000" probability="0.04" departSpeed="random"/>
    <flow id="flow_bus_NS" type="bus" route="route_NS_0" begin="0" end="1000" probability="0.04" departSpeed="random"/>
    <flow id="flow_bus_EW" type="bus" route="route_EW_0" begin="0" end="1000" probability="0.03" departSpeed="random"/>
    
    <!-- Bikes - 10% of traffic -->
    <flow id="flow_bike_EW" type="bike" route="route_EW_2" begin="0" end="1000" probability="0.03" departSpeed="random"/>
    <flow id="flow_bike_SN" type="bike" route="route_SN_1" begin="0" end="1000" probability="0.03" departSpeed="random"/>
    <flow id="flow_bike_WE" type="bike" route="route_WE_3" begin="0" end="1000" probability="0.02" departSpeed="random"/>
    
    <!-- Motorcycles - 10% of traffic -->
    <flow id="flow_moto_NS" type="motorcycle" route="route_NS_2" begin="0" end="1000" probability="0.03" departSpeed="random"/>
    <flow id="flow_moto_EW" type="motorcycle" route="route_EW_3" begin="0" end="1000" probability="0.03" departSpeed="random"/>
    
    <!-- Trucks - 15% of traffic -->
    <flow id="flow_truck_WE" type="truck" route="route_WE_1" begin="0" end="1000" probability="0.04" departSpeed="random"/>
    <flow id="flow_truck_NS" type="truck" route="route_NS_1" begin="0" end="1000" probability="0.04" departSpeed="random"/>
    
    <!-- Additional traffic for heavy congestion -->
    <flow id="flow_cars_complex_3" type="car" route="route_complex_3" begin="0" end="1000" probability="0.08"/>
    <flow id="flow_cars_complex_4" type="car" route="route_complex_4" begin="0" end="1000" probability="0.08"/>
    
    <!-- 7 Ambulances (Staggered spawn times for clear demonstration) -->
    <vehicle id="ambulance_1" type="ambulance" route="ambulance_route_1" depart="0" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_2" type="ambulance" route="ambulance_route_2" depart="20" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_3" type="ambulance" route="ambulance_route_3" depart="40" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_4" type="ambulance" route="ambulance_route_4" depart="60" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_5" type="ambulance" route="ambulance_route_5" depart="80" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_6" type="ambulance" route="ambulance_route_6" depart="100" color="1,0,0" departSpeed="max"/>
    <vehicle id="ambulance_7" type="ambulance" route="ambulance_route_7" depart="120" color="1,0,0" departSpeed="max"/>
    
</routes>
